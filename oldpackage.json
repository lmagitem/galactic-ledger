{
  "name": "galactic-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch:galactic-scanner": "cd ../galactic-scanner/ && cargo watch -x 'run --verbose'",
    "dev": "vite",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./",
    "build": "tsc && vite build --emptyOutDir",
    "build:watch": "chokidar './src/**/*.{js,jsx,ts,tsx,css,html,json}' -c 'npm run build'",
    "fullstack": "npm-run-all --parallel build:watch watch:galactic-scanner"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@tippyjs/react": "^4.2.6",
    "mathjs": "^12.0.0",
    "object-hash": "^3.0.0",
    "pixi-viewport": "^4.38.0",
    "pixi.js": "^6.5.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "babel-loader": "^9.1.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
